{
  "name": "vitalInterpreterApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "eas-build-pre-install": "npm config set legacy-peer-deps true",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "start:development": "NODE_ENV=development npx react-native start --reset-cache",
    "start:qa": "NODE_ENV=qa npx react-native start --reset-cache",
    "start:uat": "NODE_ENV=uat npx react-native start --reset-cache",
    "test:unit": "jest",
    "test:e2e:build:ci": "detox build --configuration ios && detox build --configuration android",
    "test:e2e:run:ci": "detox test --configuration ios && detox test --configuration android",
    "test:e2e:build:ios": "detox build --configuration ios",
    "test:e2e:build:android": "detox build --configuration android",
    "test:e2e:run:ios": "detox test --configuration ios --cleanup",
    "test:e2e:run:android": "detox test --configuration android  --cleanup",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.17.11",
    "@react-native-community/cli-platform-android": "^10.2.0",
    "@react-native-community/datetimepicker": "^6.7.3",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-navigation/bottom-tabs": "^6.3.2",
    "@react-navigation/native": "^6.0.11",
    "@react-navigation/native-stack": "^6.9.8",
    "@types/node": "^18.7.11",
    "@types/react-native-vector-icons": "^6.4.11",
    "axios": "^0.27.2",
    "date-fns": "^2.29.3",
    "i18next": "^21.10.0",
    "i18next-xhr-backend": "^3.2.2",
    "jest-transform-stub": "^2.0.0",
    "jwt-decode": "^3.1.2",
    "msal": "^1.4.17",
    "pre-commit": "^1.2.2",
    "query-string": "^7.1.3",
    "react": "18.0.0",
    "react-aad-msal": "^2.3.5",
    "react-i18next": "^11.18.6",
    "react-native": "0.69.7",
    "react-native-app-auth": "^6.4.3",
    "react-native-azure-auth": "^1.8.7",
    "react-native-config": "^1.4.11",
    "react-native-gesture-handler": "^2.5.0",
    "react-native-keychain": "^8.1.1",
    "react-native-localize": "^2.2.2",
    "react-native-modal-datetime-picker": "^14.0.1",
    "react-native-paper": "^5.0.0-rc.9",
    "react-native-paper-dates": "^0.8.7",
    "react-native-reanimated": "^2.14.4",
    "react-native-safe-area-context": "^4.3.1",
    "react-native-screens": "^3.15.0",
    "react-native-vector-icons": "^9.2.0",
    "react-navigation": "^4.4.4",
    "ts-jest": "^28.0.7",
    "use-http": "^1.0.27"
  },
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@tsconfig/react-native": "^2.0.0",
    "@types/jest": "^28.1.6",
    "@types/react-native": "^0.69.1",
    "@types/react-native-dotenv": "^0.2.0",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "babel-jest": "^28.1.3",
    "detox": "^19.7.1",
    "eslint": "^7.32.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "jest": "^28.1.2",
    "lint-staged": "^13.0.3",
    "metro-react-native-babel-preset": "^0.70.3",
    "react-native-dotenv": "^3.4.8",
    "react-test-renderer": "18.0.0",
    "typescript": "^4.4.4"
  },
  "resolutions": {
    "@types/react": "^18"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ]
  }
}
